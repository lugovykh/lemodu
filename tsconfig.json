{
  "compilerOptions": {
    "strict": true,
    "noPropertyAccessFromIndexSignature": true,
    "target": "ESNext",
    "module": "ESNext",
    "lib": [
      "ESNext",
      "DOM"
    ],
    "removeComments": true,
    "sourceMap": true
  },
  "include": [
    "**/*"
  ],
  "exclude": [
    "**/node_modules",
    "./app",
  ]
}
